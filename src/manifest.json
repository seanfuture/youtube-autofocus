{
	"name": "YouTube Auto Focus",
	"version": "0.5",
	"manifest_version" : 2,
	"description": "Automatically focuses YouTube's HTML5 video player for instant keyboard navigation",
    "content_scripts": [
	{
		"matches": ["http://*.youtube.com/*", "https://*.youtube.com/*"],
		"js": ["main.js"],
		"run_at": "document_idle"
	}
	],
	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},
	"homepage_url": "https://github.com/seanfuture/youtube-autofocus"
}
